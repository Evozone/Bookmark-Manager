{
    "env": {
        "node": true,
        "browser": true,
        "jest": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "import"
    ],
    "rules": {
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error",
        "no-unused-vars": "warn",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "18.1.0"
        }
    }
}